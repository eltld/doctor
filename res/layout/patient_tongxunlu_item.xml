<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    <TextView
        android:id="@+id/patient_tongxunlu_catalog"
        style="@style/MMListCatalog"
        android:text="A"
        android:layout_height="wrap_content"
        android:paddingRight="4dp" />

    <LinearLayout
        android:id="@+id/patient_tongxunlu_layout"
        style="@style/MMListItem"
        android:layout_height="56dp"
        android:background="@drawable/list_item_default"
        android:paddingLeft="8dp" >

       
		<net.ememed.doctor2.widget.CircleImageView
		    android:id="@+id/patient_tongxunlu_head_img"
		    android:layout_width="37dp"
		    android:layout_height="37dp"
		    android:background="@drawable/avatar_large"
		    android:layout_marginLeft="5dp"
		    />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:orientation="vertical"
            android:paddingLeft="18dp" >

            <TextView
                android:id="@+id/patient_tongxunlu_nick"
                android:text="张丽丽"
                style="@style/MMFontTitleInList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true" />	
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
